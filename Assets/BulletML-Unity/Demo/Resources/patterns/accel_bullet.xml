<?xml version="1.0" ?>
<!DOCTYPE bulletml SYSTEM "bulletml.dtd">
<bulletml>

  <action label="top">
    <action>
      <repeat>
        <times>99999</times>
        <action>
          <!-- Curve shot-->
          <fire>
            <direction>0</direction>
            <speed>1</speed>
            <bullet label="straight">
            <changeSpeed>
  				<speed>10</speed>
 				<term>60</term>
			</changeSpeed>

            </bullet>
          </fire>
          <wait>60</wait>
        </action>
      </repeat>
    </action>
  </action>


</bulletml>
